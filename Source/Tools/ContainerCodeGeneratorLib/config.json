{
    "targetForParsing": {
        "directories": [
            "./Sources"
        ],
        "recursive": true
    },
    "filter": {
        "attribute": "REFLECTION_ATTRIBUTE",
        "extensions": [
            ".h",
            ".hpp",
            ".hh",
            ".hxx",
            ".h++"
        ],
        "inheritances": [],
        "memberIgnore": ""
    },
    "targetForCodeGeneration": {
        "directory": "./Sources/GeneratedFiles",
        "includeListParams": {
            "includeListFileName": "IncludeList",
            "dirToInclude": []
        },
        "header": "\tReflectionCodeGenerator",
        "typeCustomizerMap": {},
        "appendTypeCustomizerMap": true,
        "implementations": {
            "JsonSerializerGenerator": {
                "exportDeclaration": "DllExport",
                "nameSpaceName": "nsContainerCodeGenerator",
                "fileName": "JsonSerializer",
                "className": "TJsonSerializer",
                "keyValueMap": {},
                "externalSources": {
                    "inFileList": [],
                    "outFile": "./Sources/GeneratedFiles/JsonOutFile.json"
                }
            }
        }
    }
}
